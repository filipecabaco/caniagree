{
  "name": "caniagree",
  "version": "0.0.1",
  "description": "Can I Agree Web application",
  "repository": {
    "type": "git",
    "url": "git://github.com/"
  },
  "engines": {
    "node": ">=8.5",
    "yarn": ">=1.0"
  },
  "author": "caniagree-team",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.dev.babel.js --color -p",
    "start": "yarn start:dev",
    "start:dev": "cross-env NODE_ENV=development APP=caniagree node server",
    "clean": "rimraf ./build/*",
    "pretest": "yarn lint",
    "lint": "eslint --ext .js src server showcase"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "env",
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "src"
        ]
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.23.0",
    "classnames": "^2.2.5",
    "history": "^3.3.0",
    "is-plain-object": "^2.0.4",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.6"
  },
  "devDependencies": {
    "ajv": "^5.2.2",
    "babel-core": "6",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.1.0",
    "compression": "^1.7.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "ejs": "^2.5.7",
    "eslint": "^4.5.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-react-app": "^2.0.0",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.2.1",
    "exports-loader": "^0.6.4",
    "express": "^4.15.4",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.2.0",
    "json-loader": "^0.5.7",
    "minimist": "^1.2.0",
    "null-loader": "^0.1.1",
    "prettier": "^1.7.0",
    "request": "^2.34",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": false
    },
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "extends": [
      "react-app",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "semi": false
        }
      ]
    }
  }
}
